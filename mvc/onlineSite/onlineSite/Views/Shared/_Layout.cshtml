<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    @Styles.Render("~/Content/css/style.css")
    @Styles.Render("~/Content/css/responsive.css")
    @Styles.Render("~/Content/css/custom.css")
</head>
<body>
   
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

    @Scripts.Render("~/Content/js/jquery-3.2.1.min.js")
    @Scripts.Render("~/Content/js/popper.min.js")
    @Scripts.Render("~/Content/js/bootstrap.min.js")
    @Scripts.Render("~/Content/js/jquery.superslides.min.js")
    @Scripts.Render("~/Content/js/bootstrap-select.js")
    @Scripts.Render("~/Content/js/inewsticker.js")
    @Scripts.Render("~/Content/js/bootsnav.js.")
    @Scripts.Render("~/Content/js/images-loded.min.js")
    @Scripts.Render("~/Content/jsisotope.min.js")
    @Scripts.Render("~/Content/jsowl.carousel.min.js")
    @Scripts.Render("~/Content/js/baguetteBox.min.js")
    @Scripts.Render("~/Content/js/form-validator.min.js")
    @Scripts.Render("~/Content/js/contact-form-script.js")
    @Scripts.Render("~/Content/js/custom.js")
</body>
</html>
