<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.8.3.js"></script>

    <link rel="icon" href="~/content/images/favicon.ico" type="image/x-icon" />
    <link rel="stylesheet" href="~/content/css/bootstrap/bootstrap.min.css">
    <link rel="stylesheet" href="~/content/css/font-awesome/all.css">
    <link rel="stylesheet" href="~/content/css/owl-carousel/owl.carousel.min.css">
    <link href="~/content/css/style.css" rel="stylesheet" type="text/css">
    <link href="~/content/css/media.css" rel="stylesheet" type="text/css">
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
               
               
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                </ul>
            </div>
        </div>
    </div>

    <div class="container body-content">
        @RenderBody()
        <hr />
        
    </div>

    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/content/js/jquery.min.js"></script>
    <script src="~/content/js/bootstrap/popper.min.js"></script>
    <script src="~/content/js/bootstrap/bootstrap.min.js"></script>
    <script src="~/content/js/custom.js"></script>
    <script src="~/content/js/owl-carousel/owl.carousel.js"></script>
</body>
</html>